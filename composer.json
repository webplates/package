{
    "name": "webplates/package",
    "description": "Webplates package",
    "license": "MIT",
    "keywords": ["webplates", "package"],
    "homepage": "http://webplates.xyz",
    "authors": [
        {
            "name": "Author",
            "email": "email@author.me"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Webplates\\Package\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "clean": "rm -rf build/ vendor/",
        "test": "echo \"No tests defined\"",
        "test-coverage": "echo \"No tests defined\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
